module _root_impl
    _s : [0..2] init 0;

    [] _operating & (_active & _s = 0) -> (_s' = 1);
    [] _operating & (_active & _s = 0) -> (_s' = 2);
endmodule

formula _operating = __loc = 1;

label "initconf" = _s > 0;

module _controller
    __loc : [0..1] init 0;
    _a    : [0..1] init 0;
    _b    : [0..1] init 0;

    [] __loc = 0 -> (__loc' = 1) & (_a' = 1);
    [] __loc = 0 -> (__loc' = 1) & (_b' = 1);
endmodule

formula _active    = true;
formula _a_active  = _a = 1;
formula _b_active  = _b = 1;

